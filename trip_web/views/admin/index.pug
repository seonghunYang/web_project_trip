extends ../layout

block content
  .container
    h1.main-title 관리자 계정


    each user in users
      .admin
        dl.row
          dt.col-md-3 Name
          dd.col-md-9= user.name 
          dt.col-md-3 Email
          dd.col-md-9
            a(href=`mailto:${user.email}`)= user.email

          dt.col-md-3 Created At
          dd.col-md-9= user.createdAt 
    hr
    p
      a.btn.btn-outline-primary(href='/admin/new') 관리자 추가
